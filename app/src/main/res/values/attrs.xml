<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="GeneralItem">
        <attr name="item_icon" format="reference"/>
        <attr name="item_text" format="string"/>
        <attr name="item_arrow" format="reference"/>
        <attr name="item_divider_top" format="boolean"/>
        <attr name="item_divider_bottom" format="boolean"/>
    </declare-styleable>

    <!--WilliamChart-->
    <declare-styleable name="ChartAttrs">

        <attr name="chart_axis" format="boolean"/>
        <attr name="chart_axisThickness" format="dimension"/>
        <attr name="chart_axisColor" format="reference|color"/>
        <attr name="chart_axisBorderSpacing" format="dimension"/>
        <attr name="chart_axisTopSpacing" format="dimension"/>

        <attr name="chart_axisLabelsSpacing" format="dimension"/>

        <attr name="chart_labels" format="enum">
            <enum name="outside" value="0"/>
            <enum name="inside" value="1"/>
            <enum name="none" value="2"/>
        </attr>

        <attr name="chart_labelColor" format="reference|color"/>
        <attr name="chart_fontSize" format="dimension"/>
        <attr name="chart_typeface" format="string"/>

        <attr name="chart_shadowColor" format="reference|color"/>
        <attr name="chart_shadowDx" format="dimension"/>
        <attr name="chart_shadowDy" format="dimension"/>
        <attr name="chart_shadowRadius" format="dimension"/>

    </declare-styleable>

    <declare-styleable name="BarChartAttrs">
        <attr name="chart_barSpacing" format="dimension"/>
        <attr name="chart_setSpacing" format="dimension"/>
        <attr name="chart_cornerRadius" format="dimension"/>
        <attr name="chart_barBackgroundColor" format="reference|color"/>
    </declare-styleable>
    <!--WilliamChart-->

    <declare-styleable name="DayPickerView">
        <attr name="colorCurrentDay" format="color"/>
        <attr name="colorSelectedDayBackground" format="color"/>
        <attr name="colorSelectedDayText" format="color"/>
        <attr name="colorPreviousDay" format="color"/>
        <attr name="colorNormalDay" format="color"/>
        <attr name="colorMonthName" format="color"/>
        <attr name="colorDayName" format="color"/>
        <attr name="textSizeDay" format="dimension"/>
        <attr name="textSizeMonth" format="dimension"/>
        <attr name="textSizeDayName" format="dimension"/>
        <attr name="headerMonthHeight" format="dimension"/>
        <attr name="selectedDayRadius" format="dimension"/>
        <attr name="calendarHeight" format="dimension"/>
        <attr name="enablePreviousDay" format="boolean"/>
        <attr name="currentDaySelected" format="boolean"/>
        <attr name="drawRoundRect" format="boolean"/>
        <attr name="firstMonth" format="enum">
            <enum name="january" value="0"/>
            <enum name="february" value="1"/>
            <enum name="march" value="2"/>
            <enum name="april" value="3"/>
            <enum name="may" value="4"/>
            <enum name="june" value="5"/>
            <enum name="july" value="6"/>
            <enum name="august" value="7"/>
            <enum name="september" value="8"/>
            <enum name="october" value="9"/>
            <enum name="november" value="10"/>
            <enum name="december" value="11"/>
        </attr>
        <attr name="lastMonth" format="enum">
            <enum name="january" value="0"/>
            <enum name="february" value="1"/>
            <enum name="march" value="2"/>
            <enum name="april" value="3"/>
            <enum name="may" value="4"/>
            <enum name="june" value="5"/>
            <enum name="july" value="6"/>
            <enum name="august" value="7"/>
            <enum name="september" value="8"/>
            <enum name="october" value="9"/>
            <enum name="november" value="10"/>
            <enum name="december" value="11"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="NotSlideViewPager">
        <attr name="canSlide" format="boolean"/>
    </declare-styleable>

    <attr name="corners" format="dimension"/>
    <attr name="topLeftRadius" format="dimension"/>
    <attr name="topRightRadius" format="dimension"/>
    <attr name="bottomLeftRadius" format="dimension"/>
    <attr name="bottomRightRadius" format="dimension"/>
    <attr name="solid" format="color"/>
    <attr name="pressed_color" format="color"/>
    <attr name="stroke_color" format="color"/>
    <attr name="stroke_width" format="dimension"/>
    <attr name="divider_color" format="color"/>
    <attr name="shape">
        <enum name="rectangle" value="0"/>
        <enum name="oval" value="1"/>
        <enum name="line" value="2"/>
        <enum name="ring" value="3"/>
    </attr>

    <declare-styleable name="ShapeTextView">
        <attr name="corners"/>
        <attr name="topLeftRadius"/>
        <attr name="topRightRadius"/>
        <attr name="bottomLeftRadius"/>
        <attr name="bottomRightRadius"/>
        <attr name="solid"/>
        <attr name="pressed_color"/>
        <attr name="stroke_color"/>
        <attr name="stroke_width"/>
        <attr name="shape"/>
        <attr name="divider_color"/>
    </declare-styleable>

    <declare-styleable name="ShapeLinearLayout">
        <attr name="corners"/>
        <attr name="topLeftRadius"/>
        <attr name="topRightRadius"/>
        <attr name="bottomLeftRadius"/>
        <attr name="bottomRightRadius"/>
        <attr name="solid"/>
        <attr name="pressed_color"/>
        <attr name="stroke_color"/>
        <attr name="stroke_width"/>
        <attr name="shape"/>
    </declare-styleable>

    <declare-styleable name="Triangle">
        <attr name="rotation" format="float"/>
        <attr name="color" format="color"/>
    </declare-styleable>


</resources>